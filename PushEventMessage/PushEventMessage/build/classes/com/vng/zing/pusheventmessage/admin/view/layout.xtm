    <style type="text/css">
        .row-button clearfix .active{
            background-color: #03a6fa;
            color:#ffff;
        }

        #icon{
            background-image:url({{app_domain}}/public/admin/img/check_white.png); 
            background-repeat: no-repeat; 
            background-position: 7px 10px;
            margin-left: 225px;
            padding-left: 30px;
            margin-top: 7px;
        }

        #app-noti {
          position: fixed;
          top: 710px;
          right: 350px;
          float: right;
          z-index: 999999;
          font-size: 1em;
      }
      #app-noti p {
          display: box;
          background-color: #4c5761;
          color: white;
          /*border: solid 1px #d14;*/
          margin: 5px;
          padding: 10px;
          width: 200px;
          border-radius: 3px;
          word-wrap: break-word;
      }
      

#icon_save{
                background-image:url({{app_domain}}/public/admin/img/check_white.png); 
                background-repeat: no-repeat; 
                background-position: 7px 10px;
                padding-left: 30px;
                margin-top: 5px;
            }
            .wrapper table tr.noBorder  {
                border-bottom: #d3dce1 solid 0px  !important;
                padding-bottom: 0px; 
            }
            .btn-file {
                position: relative;
                overflow: hidden;
            }
            .btn-file input[type=file] {
                position: absolute;
                top: 0;
                right: 0;
                min-width: 100%;
                min-height: 100%;
                font-size: 100px;
                font-weight:bold;
                text-align: right;
                filter: alpha(opacity=0);
                opacity: 0;
                outline: none;
                background: white;
                cursor: inherit;
                display: block;
            }
            .wrapper .list-noti li .list-control li .non-float{
                float:none;
            }
            
        .showhint{
            display: block !important;
        }

        .tag-item{
            padding:0px !important;
            width:auto !important;
        }

        
div.xdsoft_calendar table {
    font-size:0.3em !important;

}
div.xdsoft_calendar table  td{
    padding:0px !important;
